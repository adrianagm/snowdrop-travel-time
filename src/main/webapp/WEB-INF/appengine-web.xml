<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>${profile.appengine.app.projectid}</application>
    <version>${appengine.app.version}</version>
    <threadsafe>true</threadsafe>
    <use-google-connector-j>true</use-google-connector-j>

    <system-properties>  
         <property name="ae-cloudsql.cloudsql-database-url" value="${profile.db.cloudsql}" />
         <property name="ae-cloudsql.local-database-url" value="${profile.db.local}" />
        
    </system-properties>
</appengine-web-app>
